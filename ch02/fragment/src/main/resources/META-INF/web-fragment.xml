<?xml version="1.0" encoding="UTF-8"?>
<web-fragment id="WebFragment_ID" version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-fragment_4_0.xsd">
    <display-name>fragment</display-name>
    <name>fragment</name>
    <servlet>
        <servlet-name>FragmentConfig</servlet-name>
        <servlet-class>com.example.fragment.FragmentConfig</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>FragmentConfig</servlet-name>
        <url-pattern>/fragmentConfig</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FragmentMainServlet</servlet-name>
        <servlet-class>cc.openhome.FragmentMainServlet</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>FragmentMainServlet</servlet-name>
        <url-pattern>/fragmentMainServlet</url-pattern>
    </servlet-mapping>
</web-fragment>